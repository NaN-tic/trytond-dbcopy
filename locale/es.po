msgctxt "model:ir.message,text:cannot_overwrite"
msgid "You cannot overwrite current database."
msgstr "No puedes sobrescribir la base de datos actual."

msgctxt "model:ir.message,text:db_cloned_successfully"
msgid "Database \"%(source)s\" cloned successfully into \"%(target)s\".\n Now you can connect to the new database"
msgstr "Base de datos \"%(source)s\" clonada con éxito en \"%(target)s\".\n Ahora puede conectarse a la nueva base de datos."

msgctxt "model:ir.message,text:connection_error"
msgid "Error connection to new test database.\n Please, create new test database or contact us."
msgstr "Error en la conexión a la nueva base de datos.\n Cree de nuevo una nueva copia o contacte con nosotros."

msgctxt "model:ir.message,text:dumping_db_error"
msgid "Error dumping database \"%(source)s\"."
msgstr "Error volcando la base de datos \"%(source)s\"."

msgctxt "model:ir.message,text:creating_db_error"
msgid "Error creating database \"%(source)s\"."
msgstr "Error creando la base de datos \"%(source)s\"."

msgctxt "model:ir.message,text:dropping_db_error"
msgid "Error dropping database \"%(source)s\"."
msgstr "Error borrando la base de datos \"%(source)s\"."

msgctxt "model:ir.message,text:restoring_db_error"
msgid "Error restoring database \"%(source)s\"."
msgstr "Error restaurando la base de datos \"%(source)s\"."

msgctxt "model:ir.message,text:email_subject"
msgid "Tryton dbcopy result of clone database \"%(source_database)s\""
msgstr "Resultado de Trytond dbcopy de clonación de la base de datos \"%(source_database)s\""

msgctxt "model:ir.message,text:user_email_error"
msgid "User \"%(user)s\" has not got any email address."
msgstr "El usuario \"%(user)s\" no tiene ninguna dirección de correo electrónico."

msgctxt "model:ir.message,text:dbname_error"
msgid "You can not clone a database from a test database. Connect to live database and clone new test database."
msgstr "No puede clonar una base de datos que el orígen ya es una copia. Connecte a la base de datos de producción y clone una nueva base de datos."

msgctxt "model:ir.message,text:dbname_scheme"
msgid "You database is not PSQL. Can not copy your database"
msgstr "La base de datos no es PSQL. No se puede copiar la base de datos"

